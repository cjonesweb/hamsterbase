= form_for @stash_tunnel do |f|
  - if @stash_tunnel.errors.any?
    #error_explanation
      %h2= "#{pluralize(@stash_tunnel.errors.count, "error")} prohibited this stash_tunnel from being saved:"
      %ul
        - @stash_tunnel.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :stash_id
    -#f.number_field :stash_id
    = collection_select :stash, :stash_id, @stashes_unpaged, :id, :title
  .field
    -#f.label :network_stash_id
    = collection_select :stash, :network_stash_id, @stashes_unpaged, :id, :title
    = f.number_field :network_stash_id
  .field
    = f.label :comment
    = f.text_area :comment
  .actions
    = f.submit 'Save'
